<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="kr.hs.dgsw.webshop.SubMenuMapper">
    <select select id="findById" parameterType="Long" resultType="kr.hs.dgsw.webshop.SubMenu">
      select id, menuid, name from submenu
      where id=#{id}
    </select>

    <select id="findByMenuid" parameterType="Long" resultType="kr.hs.dgsw.webshop.SubMenu">
      select id, menuid, name from submenu
      where menuid=#{menuid}
    </select>
  </mapper>