<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="kr.hs.dgsw.webshop.MenuMapper">
    <select id="findall" resultType="kr.hs.dgsw.webshop.Menu">
      select id, name from menu
    </select>

    <select id="findById" parameterType="Long" resultType="kr.hs.dgsw.webshop.Menu">
      select id, name from menu
      where id=#{id}
    </select>
  </mapper>